{
  "name": "mantine-minimal-next-template",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --port 3000",
    "stop": "pkill -f 'next dev'",
    "build": "npx prisma generate && next build",
    "start": "npx prisma generate && next start",
    "type-check": "tsc --noEmit",
    "db:reset": "npx supabase db reset && yarn prisma:migrate && yarn supabase:generate-types",
    "biome:format": "biome format --write src",
    "biome:lint": "biome lint --apply src",
    "biome:check": "biome check --apply src",
    "prisma:migrate": "npx prisma migrate dev && npx prisma db seed && npx prisma generate",
    "prisma:studio": "npx prisma studio",
    "prisma:seed": "npx prisma db seed",
    "supabase:start": "npx supabase start",
    "supabase:stop": "npx supabase stop",
    "supabase:status": "npx supabase status",
    "supabase:reset": "npx supabase db reset && yarn supabase:generate-types",
    "supabase:link": "npx supabase link",
    "supabase:generate-types": "npx supabase gen types --lang=typescript --local > src/types/supabase.d.ts",
    "supabase:generate-migration": "npx supabase db diff | npx supabase migration new",
    "supabase:generate-seed": "npx supabase db dump --data-only -f supabase/seed.sql",
    "supabase:migration-new": "npx supabase migration new",
    "supabase:push": "npx supabase db push",
    "supabase:pull": "npx supabase db pull"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  },
  "dependencies": {
    "@mantine/carousel": "7.5.1",
    "@mantine/charts": "7.5.1",
    "@mantine/core": "7.5.1",
    "@mantine/dates": "7.5.1",
    "@mantine/dropzone": "^7.13.0",
    "@mantine/form": "7.5.1",
    "@mantine/hooks": "7.5.1",
    "@mantine/notifications": "7.5.1",
    "@prisma/client": "^5.20.0",
    "@supabase/ssr": "^0.5.1",
    "@supabase/supabase-js": "^2.45.4",
    "@tabler/icons-react": "^2.47.0",
    "dayjs": "^1.11.10",
    "next": "^14.2.13",
    "nuqs": "^1.19.3",
    "prisma": "^5.20.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.2",
    "@types/node": "^22.7.4",
    "@types/react": "18.2.36",
    "@types/react-dom": "18.2.14",
    "eslint": "8.53.0",
    "eslint-config-next": "14.0.1",
    "postcss": "^8.4.31",
    "postcss-preset-mantine": "1.13.0",
    "postcss-simple-vars": "^7.0.1",
    "ts-node": "^10.9.2",
    "typescript": "5.2.2"
  },
  "packageManager": "yarn@4.0.1"
}
